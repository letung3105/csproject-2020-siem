<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="vn.edu.vgu.jupiter.dashboard.Dashboard">
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                  prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
         <children>
            <TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                     prefHeight="544.0" prefWidth="300.0" tabClosingPolicy="UNAVAILABLE"
                     AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                     AnchorPane.topAnchor="0.0">
               <tabs>
                  <Tab text="PortScanAlerts">
                     <content>
                        <AnchorPane>
                           <children>
                              <Accordion prefWidth="300.0" AnchorPane.bottomAnchor="56.0"
                                         AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                         AnchorPane.topAnchor="0.0">
                                 <panes>
                                    <TitledPane animated="false" text="Vertical port scan's thresholds">
                                       <content>
                                          <GridPane vgap="8.0">
                                             <columnConstraints>
                                                <ColumnConstraints percentWidth="80.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0"
                                                                   minWidth="16.0" prefWidth="65.0"/>
                                             </columnConstraints>
                                             <rowConstraints>
                                                <RowConstraints vgrow="SOMETIMES"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="ALWAYS"/>
                                             </rowConstraints>
                                             <children>
                                                <Label text="Minimum count"/>
                                                <TextField fx:id="verticalMinCount"
                                                           GridPane.columnIndex="1"/>
                                                <Label text="Time window (seconds)"
                                                       GridPane.rowIndex="1"/>
                                                <Label text="Alert interval (seconds)"
                                                       GridPane.rowIndex="2"/>
                                                <TextField fx:id="verticalTime" GridPane.columnIndex="1"
                                                           GridPane.rowIndex="1"/>
                                                <TextField fx:id="verticalTimeInterval"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="2"/>
                                                <Label text="High priority threshold"
                                                       GridPane.rowIndex="3"/>
                                                <TextField fx:id="verticalWarnCount"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="3"/>
                                             </children>
                                          </GridPane>
                                       </content>
                                    </TitledPane>
                                    <TitledPane animated="false" text="Horizontal port scan's thresholds">
                                       <content>
                                          <GridPane vgap="8.0">
                                             <columnConstraints>
                                                <ColumnConstraints percentWidth="80.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0"
                                                                   minWidth="16.0" prefWidth="65.0"/>
                                             </columnConstraints>
                                             <rowConstraints>
                                                <RowConstraints vgrow="SOMETIMES"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="ALWAYS"/>
                                             </rowConstraints>
                                             <children>
                                                <Label text="Minimum count"/>
                                                <TextField fx:id="horizontalMinCount"
                                                           GridPane.columnIndex="1"/>
                                                <Label text="Time window (seconds)"
                                                       GridPane.rowIndex="1"/>
                                                <Label text="Alert interval (seconds)"
                                                       GridPane.rowIndex="2"/>
                                                <TextField fx:id="horizontalTime"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="1"/>
                                                <TextField fx:id="horizontalTimeInterval"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="2"/>
                                                <Label text="High priority threshold"
                                                       GridPane.rowIndex="3"/>
                                                <TextField fx:id="horizontalWarnCount"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="3"/>
                                             </children>
                                          </GridPane>
                                       </content>
                                    </TitledPane>
                                    <TitledPane animated="false" text="Block port scan's thresholds">
                                       <content>
                                          <GridPane vgap="8.0">
                                             <columnConstraints>
                                                <ColumnConstraints percentWidth="80.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="80.0"
                                                                   minWidth="16.0" prefWidth="65.0"/>
                                             </columnConstraints>
                                             <rowConstraints>
                                                <RowConstraints vgrow="SOMETIMES"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints vgrow="NEVER"/>
                                                <RowConstraints prefHeight="30.0" vgrow="NEVER"/>
                                                <RowConstraints vgrow="ALWAYS"/>
                                                <RowConstraints vgrow="ALWAYS"/>
                                             </rowConstraints>
                                             <children>
                                                <Label text="Minimum port count"/>
                                                <TextField fx:id="blockMinPortCount"
                                                           GridPane.columnIndex="1"/>
                                                <Label text="Alert Interval (seconds)"
                                                       GridPane.rowIndex="3"/>
                                                <Label text="Time window (seconds)"
                                                       GridPane.rowIndex="2"/>
                                                <TextField fx:id="blockMinAddressCount"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="1"/>
                                                <TextField fx:id="blockTime" GridPane.columnIndex="1"
                                                           GridPane.rowIndex="2"/>
                                                <Label text="High priority threshold"
                                                       GridPane.rowIndex="4"/>
                                                <TextField fx:id="blockTimeInterval"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="3"/>
                                                <Label text="Minumum address count"
                                                       GridPane.rowIndex="1"/>
                                                <TextField fx:id="blockWarnAddressCount"
                                                           GridPane.columnIndex="1"
                                                           GridPane.rowIndex="4"/>
                                             </children>
                                          </GridPane>
                                       </content>
                                    </TitledPane>
                                 </panes>
                              </Accordion>
                              <Button alignment="CENTER" mnemonicParsing="false"
                                      onAction="#refreshPortScansAlertParameter" text="Button"
                                      AnchorPane.bottomAnchor="16.0" AnchorPane.leftAnchor="120.0"
                                      AnchorPane.rightAnchor="120.0"/>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
                  <Tab text="HTTPAlerts">
                     <content>
                        <AnchorPane>
                           <fx:include fx:id="httpAlertControlPanel" source="HTTPAlertControlPanel.fxml"
                                       AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                       AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                     </content>
                  </Tab>
               </tabs>
            </TabPane>
         </children>
      </AnchorPane>
      <TextArea editable="false" prefHeight="600.0" prefWidth="500.0" text="LOG MESSAGES"
                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="300.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
         </padding>
      </TextArea>
   </children>
</AnchorPane>
