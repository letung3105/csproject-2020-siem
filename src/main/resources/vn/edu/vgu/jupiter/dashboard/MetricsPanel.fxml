                                    <VBox.margin>
                                        <Insets top="8.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="numberOfVerticalPortScanAlertsLabel" text="#events: 0">
                                    <VBox.margin>
                                        <Insets left="8.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Label>
                                <Label text="HorizontalPortScanAlert">
                                    <VBox.margin>
                                        <Insets top="8.0"/>
