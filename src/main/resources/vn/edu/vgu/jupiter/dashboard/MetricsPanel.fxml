            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Label text="NO METRIC" AnchorPane.leftAnchor="8.0" AnchorPane.topAnchor="8.0">
                            <VBox.margin>
                                <Insets top="8.0"/>
                            </VBox.margin>
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
