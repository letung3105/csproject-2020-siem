<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="vn.edu.vgu.jupiter.dashboard.StartupConfig">
    <children>
        <GridPane layoutX="114.0" layoutY="110.0" prefHeight="136.0" prefWidth="367.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="21.0" prefWidth="121.0" text="Net Device     " textAlignment="CENTER"/>
                <Label text="Apache Log Location" GridPane.rowIndex="1"/>
                <TextField fx:id="apacheLogLocationField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
